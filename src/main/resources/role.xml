<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.ce.mybatis.RoleMapper">
	<select id="getRole" parameterType="long" resultType="org.ce.mybatis.Role">
		select id,username,password from sys.end_user where id = #{id}
	</select>

</mapper>